{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn install:pytorch-core-deps && docusaurus build",
    "clear": "docusaurus clear",
    "codegen-examples": "node scripts/codegen-examples.js",
    "deploy": "yarn install:pytorch-core-deps && docusaurus deploy",
    "docusaurus": "docusaurus",
    "install:pytorch-core-deps": "(cd ../react-native-pytorch-core && yarn install)",
    "lint": "eslint .",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@docusaurus/core": "2.1.0",
    "@docusaurus/plugin-client-redirects": "2.1.0",
    "@docusaurus/plugin-google-gtag": "2.1.0",
    "@docusaurus/preset-classic": "2.1.0",
    "@docusaurus/remark-plugin-npm2yarn": "2.1.0",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "docusaurus-plugin-internaldocs-fb": "1.1.0",
    "file-loader": "^6.2.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-github-btn": "^1.3.0",
    "react-router-dom": "5.3.3",
    "rehype-katex": "^5.0.0",
    "remark-math": "3",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "docusaurus-plugin-typedoc": "^0.14.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.9.0",
    "typedoc-plugin-param-names": "^2.0.0",
    "typescript": "4.2.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "bracketSameLine": true,
          "bracketSpacing": false,
          "singleQuote": true,
          "trailingComma": "all",
          "arrowParens": "avoid"
        }
      ],
      "no-shadow": "off",
      "@typescript-eslint/no-shadow": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build/",
    "static/examples/",
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "bracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "shelljs": "^0.8.5",
    "ansi-html": "0.0.8"
  }
}
